{
  "name": "money-book",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 1995",
    "server": "nodemon backend/server.js",
    "start:backend": "npm run server",
    "start:frontend": "npm run dev",
    "docker:start": "docker-compose -f backend/docker-compose.yaml up -d",
    "docker:stop": "docker-compose -f backend/docker-compose.yaml down",
    "start:all": "npm run docker:start && sleep 10 && concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "dependencies": {
    "express": "^4.19.1",
    "pg": "^8.11.3",
    "pinia": "^2.1.7",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "vite": "^5.1.6"
  }
}
